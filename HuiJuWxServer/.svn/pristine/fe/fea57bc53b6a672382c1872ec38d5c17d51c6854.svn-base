<div class="weui_cells_title">当前锁蓝牙钥匙ID：{{deviceId}}</div>
<div class="weui_cells" ng-repeat="(k, v) in keyList">
    <div class="weui_cell" >
        <div class="weui_cell_bd weui_cell_primary">
            KeyId
        </div>
        <div class="weui_cell_ft">{{k}}</div>
    </div>
    <div class="weui_cell" >
        <div class="weui_cell_bd weui_cell_primary">
            Position
        </div>
        <div class="weui_cell_ft">{{v.position}}</div>
    </div>
    <div class="weui_cell" >
        <div class="weui_cell_bd weui_cell_primary">
            Status
        </div>
        <div class="weui_cell_ft">{{v.status}}</div>
    </div>
    <div class="weui_cell" >
        <button ng-click="deleteKey(k)">删除钥匙</button>
        <button ng-click="enableKey(k)">启用钥匙</button>
        <button ng-click="disableKey(k)">停用钥匙</button>
    </div>
</div>