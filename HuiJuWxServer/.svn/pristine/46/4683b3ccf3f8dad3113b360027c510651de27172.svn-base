 
<div id="config" class="mui-page">
	<div class="mui-navbar-inner mui-bar mui-bar-nav">
		<button type="button" class="mui-left mui-action-back mui-btn  mui-btn-link mui-btn-nav mui-pull-left">
			<span class="mui-icon mui-icon-left-nav"></span>锁列表
		</button>
		<h1 class="mui-center mui-title">扫描蓝牙设备</h1>
	</div>
	<div class="mui-page-content">
		<div class="mui-scroll-wrapper backgroundColor">
			<div class="mui-scroll backgroundColor setting-buttons config-width">
					 <button type="button" class="mui-btn mui-btn-blue mui-btn-outlined " ng-click="scanDevice()">开始扫描</button>
  					<button type="button" class="mui-btn mui-btn-blue mui-btn-outlined " ng-click="stopDevice()">停止扫描</button>
  					  <div class="mui-collapse-content">
			     <ul class="mui-table-view" ng-repeat="key in keyList">
			       <li class="mui-table-view-cell ">
			          <div class="mui-slider-right mui-disabled">
						 <button class="mui-btn mui-btn-blue " ng-click="connect(key)">连接</button>
					 </div>
			           <a    class=' mui-slider-handle mui-navigate-right'>
                                   <div class="mui-media-body">
                                        	{{key.deviceId}}
                                   </div>
                               </a>
			       </li>
			     </ul>
			  </div>
			</div>
			 
		</div>
	</div>
</div>