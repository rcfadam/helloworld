  <div class="container-fluid">
	<div class="row-fluid">
		<div class="span12" ng-repeat="device in deviceList">
			<div class="accordion" id="accordion-item" >
				<div class="accordion-group lock-item">
					<div class="accordion-heading panel-title ">
							 <div>
							 <div class="lock-item" ng-touchmove >
							    <span>芯</span>
							 <div class="">
							   <h3>锁芯名称:{{device.locktb.lockName}}</h3>
							     <p>{{device.locktb.lockId}}</p>
							   <p><strong>{{device.state}}</strong></p>
							 </div>
							 </div>
							<a class="accordion-toggle collapsed btn btn-default" data-toggle="collapse" data-parent="#accordion-item"  href="#{{device.lockId}},{{device.show}}" ng-zhankai>{{device.show?'收起':'展开'}}</a>
						 </div>
						  <div class="btn-group item-buttons" id="key.keyId">
    						<button type="button" class="btn edit-btn" ng-click="addKey(device.keyList, device)">添加钥匙</button>
    						<button type="button" class="btn btn-default" ng-click="unbindLock(device.keyList, device)">解绑锁</button>
						  </div>  
					</div>
					<div id="{{device.lockId}}" class="accordion-body collapse">
						<div class="accordion-inner">
							 <ul class="list-group key-list" ng-repeat="key in device.keyList">
							   <li class="list-group-item" >
							     <div class="key-item"  ng-touchmove>
							     <span>钥</span>
							      <div >
								    <h4>钥匙名称：{{key.keyName}}</h4>
									<p>钥匙编号:{{key.keyId}}</p>
									<p>蓝牙钥匙编号:{{key.deviceId}}</p>
									<p>钥匙状态：<strong>{{key.stateStr}}</strong> &nbsp;&nbsp;&nbsp;<strong>{{key.connectStatus}}</strong> </p>
								  </div>	
							     </div>
							   <div class="btn-group item-buttons" id="key.keyId">
			                        <button type="button" class="btn "ng-click="keyItem(device,key)" >详情</button>
								    <button type="button" class="btn "ng-click="enableKey(device,key)" ng-show="key.state!=1">启用</button>
			                        <button type="button" class="btn "ng-click="disableKey(device,key)" ng-hide="key.state!=1">禁用</button>
							        <button type="button" class="btn edit-btn"ng-click="deleteKey(device,key)">删除</button>
								</div>
							   </li>
							 </ul>
						</div>
					</div>
				</div>
			</div>
		</div>  
		</div></div>