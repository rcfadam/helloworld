 <div class="container-fluid">
	<div class="row-fluid">
		<div class="span12" ng-repeat="device in deviceList">
			<div class="accordion" id="accordion-item" >
				<div class="accordion-group lock-item">
					<div class="accordion-heading panel-title ">
						 <a class="accordion-toggle collapsed " data-toggle="collapse" data-parent="#accordion-item" href="#accordion-content" ng-click="toggleGroup(device)"  ng-class="{active: isGroupShown(device)}">
							 <span>lockId:{{device.lockId}}</span><span>{{device.state}}</span>
						 </a>
						 <i class="icon" ng-class="isGroupShown(device) ? 'ion-minus' : 'ion-plus'"></i>
						 <div class="btn-group" id="key.keyId">
    						<button type="button" class="btn btn-default" ng-click="addKey(device.keyList, device)">添加钥匙</button>
    						<button type="button" class="btn btn-default" ng-click="unbindLock(device.keyList, device)">解绑锁</button>
						  </div>
					</div>
					<div id="accordion-content" class="accordion-body collapse"  ng-show="isGroupShown(device)">
						<div class="accordion-inner">
							 <ul class="list-group key-list" ng-repeat="key in device.keyList">
							   <li class="list-group-item">
							   <div>钥匙编号:{{key.keyId}}</div><div>蓝牙钥匙编号{{key.deviceId}}</div><div>钥匙名称：{{key.keyName}}</div><div>钥匙状态：{{key.stateStr}}</div>
							   <div class="btn-group" id="key.keyId">
    							<button type="button" class="btn btn-default" ng-click="deleteKey(device,key)">删除</button>
								 <button type="button" class="btn btn-default" ng-click="enableKey(device,key)" ng-show="key.state!=1">启用</button>
								 <button type="button" class="btn btn-default" ng-click="disableKey(device,key)" ng-hide="key.state!=1">禁用</button>
								</div>
							   </li>
							 </ul>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>