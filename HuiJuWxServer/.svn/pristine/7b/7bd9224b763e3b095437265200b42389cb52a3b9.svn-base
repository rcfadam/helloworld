<html ng-app="MyApp">
<head>
<meta charset="UTF-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1, user-scalable=no">
<meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate" />
<meta http-equiv="Pragma" content="no-cache" />
<meta http-equiv="Expires" content="0" />
<link  href="dist/css/mui.min.css" rel="stylesheet"/>
<!-- <link  href="css/style.css" rel="stylesheet"/> -->
<title>小汪智能锁</title>
 <style>
 .lock-button{height:59px!important;}
 /* .mui-table-view-cell>.mui-slider-left>.mui-btn,
  .mui-table-view-cell>.mui-slider-right>.mui-btn
 {padding: 0px 10px!important;} */
 .mui-edit{
			color:#FFFFFF!important;
		 	background-color:#EC971F;
		}
		.mui-lock,.mui-key{
			padding:3px 8px;
			border-radius:50px;
		}
		.connect-status{
			color:green;
		}
		.unconnect-status{
			color:red;
		}
		.mui-scroll-wrapper{
		  overflow: scroll!important;
		}
		.lock{
  		width:30%;
  		left:1em;
  	}
  	.setting-buttons{
			 
				margin-top:20%;
				margin-left:10px;
				height:75%;
			}
			.backgroundColor{
			  background-color:#ffffff;
			}
			.config-width{
			 width:97%;
			}
 </style>
</head>

<body ng-controller="mainController">
 <!-- 缩放式侧滑（类手机QQ） -->
		<!-- 侧滑导航根容器 -->
	<div class="mui-off-canvas-wrap mui-draggable mui-scalable">
	  <!-- 菜单容器 -->
	  <aside class="mui-off-canvas-left mui-transitioning" id="offCanvasSide">
	    <div ui-view="setting"></div>
	  </aside>
	  <!-- 主页面容器 -->
	  <div class="mui-inner-wrap mui-transitioning">
	    <!-- 主页面标题 -->
	      <div ui-view="content"></div>
	    <div class="mui-off-canvas-backdrop"></div>
	  </div>
	</div> 
	<div ui-view></div>
 	<div id="forward" class="mui-popover mui-popover-action mui-popover-bottom">
		 <ul class="mui-table-view" ng-repeat="button in buttons">
	<li class="mui-table-view-cell" >
		<a   ng-click="execute(button.funcName,params1,params2)">{{button.title}}</a>
	</li>
</ul>
</div>
<!-- <ul class="mui-table-view">
	<li class="mui-table-view-cell">
		<a href="#forward"><b>取消</b></a>
	</li>
</ul> -->
 <script type="text/javascript" src="../libs/jquery-3.1.0.min.js"></script>
 <script type="text/javascript" src="dist/js/mui.min.js"></script>
 <script type="text/javascript" src="../libs/angular/angular.min.js"></script>
 <script type="text/javascript" src="../libs/angular-ui/angular-ui-router.min.js"></script>
 <script src="https://res.wx.qq.com/open/js/jweixin-1.0.0.js"></script>
<script type="text/javascript" src="../libs/toaster.min.js"></script>
<script type="text/javascript" src="dist/js/mui.view.js"></script>
 <script type="text/javascript" src="js/app1.js?v=1.7"></script>
 <script type="text/javascript" src="js/common.js?v=1.7"></script>
 <script type="text/javascript" src="js/controllers1.js?v=2.6"></script>
 <script type="text/javascript" src="js/services1.js?v=2.4"></script>
</body>
</html>