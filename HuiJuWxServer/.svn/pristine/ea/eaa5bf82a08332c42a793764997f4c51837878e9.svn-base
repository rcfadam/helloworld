<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huiju.weixin.WeixinServer.mapper.TempTbMapper">
  <resultMap id="BaseResultMap" type="com.huiju.weixin.WeixinServer.model.TempTb">
    <result column="openid" jdbcType="VARCHAR" property="openid" />
    <result column="keyid" jdbcType="VARCHAR" property="keyid" />
    <result column="lockid" jdbcType="VARCHAR" property="lockid" />
     <association property="keyTb" resultMap="com.huiju.weixin.WeixinServer.mapper.KeyTbMapper.BaseResultMap">
     </association>
  </resultMap>
  <insert id="insert" parameterType="com.huiju.weixin.WeixinServer.model.TempTb">
    insert into temp_tb (openid, keyid, lockid
      )
    values (#{openid,jdbcType=VARCHAR}, #{keyid,jdbcType=VARCHAR}, #{lockid,jdbcType=VARCHAR}
      )
  </insert>
  <insert id="insertSelective" parameterType="com.huiju.weixin.WeixinServer.model.TempTb">
    insert into temp_tb
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="openid != null">
        openid,
      </if>
      <if test="keyid != null">
        keyid,
      </if>
      <if test="lockid != null">
        lockid,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="openid != null">
        #{openid,jdbcType=VARCHAR},
      </if>
      <if test="keyid != null">
        #{keyid,jdbcType=VARCHAR},
      </if>
      <if test="lockid != null">
        #{lockid,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
  <select id="findTempTbByOpenId" resultMap="BaseResultMap" >
    select keytb.* from temp_tb temptb inner join key_tb keytb on keytb.key_id = temptb.keyid 
    where temptb.openid = #{openId}
  </select>
  <delete id="deleteByOpenIdAndKeyId" >
    delete from temp_tb where openid = #{openId} and keyid = #{keyId}
  </delete>
</mapper>